# editorconfig.org

# This is the project's root directory
root = true

[*]
insert_final_newline = true

[*.{json,yml,yml.dist}]
indent_style = space
indent_size = 2
trim_trailing_whitespace = true

[{Makefile,**.mk}]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = tab
